  procedures :
   -
     name : UPDTask
     procedure_id : 1653512750606969587
     type_code : 14
     code : |1+
      PROCEDURE UPDTask()
      
      // Modification?
      IF gnTaskID <> 0 THEN
      	HReadSeek(TASK,TASKID,gnTaskID)
      	IF NOT HFound() THEN RETURN
      ELSE
      	HReset(TASK)
      END
      	
      // Retrieve the new data and perform a check
      TASK.STARTDATE    = EDT_START_DATE + EDT_START_TIME
      TASK.ENDDATE      = EDT_END_DATE + EDT_END_TIME
      TASK.STCTASK     = EDT_Task
      TASK.TASKSTATUS    = RADIO_TaskStatus
      TASK.TASKCONTENT = EDT_CONTENT
      TASK.WHOLEDAY   = CBOX_WholeDay
      
      IF COMBO_Person > 1 THEN
      	TASK.PERSONID = COMBO_Person..Value
      ELSE
      	Error(<§$0003§>)
      	ReturnToCapture(COMBO_Person)
      END
      
      IF COMBO_Type > 1 THEN
      	TASK.TYPEID = COMBO_Type..Value
      ELSE
      	Error(<§$0004§>)
      	ReturnToCapture(COMBO_Type)
      END
      
      IF COMBO_Location > 1 THEN
      	TASK.LOCATIONID = COMBO_Location..Value
      ELSE
      	TASK.LOCATIONID = 0
      END
      
      IF gnTaskID <> 0 THEN
      	// Record
      	HModify(TASK)
      	IF HErrorIntegrity() THEN
      		Error(HErrorInfo())
      		ReturnToCapture()
      	END	
      ELSE
      	HAdd(TASK)
      END
     type : 458752
   -
     name : InputCheck
     procedure_id : 1653512750607035123
     type_code : 14
     code : |1-
      PROCEDURE InputCheck()
      
      // Checks whether the main controls are filled
      IF NOT DateValid(EDT_START_DATE) THEN
      	Error(<§$000c§>)
      	RESULT False
      END
      
      IF NOT DateValid(EDT_END_DATE) THEN
      	Error(<§$000d§>)
      	RESULT False
      END
      
      IF NOT TimeValid(EDT_START_TIME) THEN
      	Error(<§$000e§>)
      	RESULT False
      END
      
      IF NOT TimeValid(EDT_END_TIME) THEN
      	Error(<§$000f§>)
      	RESULT False
      END
      
      IF EDT_Task ~= "" THEN
      	Error(<§$0010§>)
      	RESULT False
      END
      
      RESULT True
     type : 458752
   -
     name : InitializeForm
     procedure_id : 1653512750607100659
     type_code : 14
     code : |1-
      // Summary: Initializes the window controls
      // Syntax:
      // InitializeForm ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      // Example:
      // Indicate an example.
      //
      PROCEDURE InitializeForm()
      
      // Variables
      dToday is date = Today()
      
      // Modification?
      IF gnTaskID <> 0 THEN
      	HReadSeekFirst(TASK,TASKID,gnTaskID)
      	dbgAssert(HFound(TASK),"Task not found")
      	
      	// Initializes the controls
      	FileToScreen(WIN_TaskForm,TASK)
      	
      	EDT_START_DATE  = TASK.STARTDATE..Date
      	EDT_END_DATE    = TASK.ENDDATE..Date
      	EDT_START_TIME = TASK.STARTDATE..Time
      	EDT_END_TIME   = TASK.ENDDATE..Time
      	
      	// Manage the tasks on a daily basis
      	IF TASK.WHOLEDAY = True THEN
      		EDT_START_TIME..State = Grayed
      		EDT_END_TIME..State = Grayed
      	END	
      	
      	// Possible deletion
      	BTN_DELETE..Visible = True
      	WIN_TaskForm..Title = "Modifying an existing task"
      ELSE
      	// Default values
      	EDT_START_DATE  = dToday ; dToday..Day += 7
      	EDT_END_DATE    = dToday
      	EDT_START_TIME = "0800"
      	EDT_END_TIME   = "1800"
      	
      	BTN_DELETE..Visible = False
      	WIN_TaskForm..Title = "New task"
      	
      	ListSelectPlus(COMBO_Location,2)
      	ListSelectPlus(COMBO_Person,2)
      	ListSelectPlus(COMBO_Type,2)
      END
      
      ExecuteProcess(COMBO_Type,trtSelection)
      WIN_TaskForm..ReturnedValue = False
     type : 458752
