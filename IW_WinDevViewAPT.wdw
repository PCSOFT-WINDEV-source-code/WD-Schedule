      -
        code : |1+
         // Procedure called before each appointment drawing in an Organizer, Scheduler or TimeLine control
         // This code is re-run for EACH appointment display
         PROCEDURE IW_WinDevViewAPT( gEventOrAPT is an Appointment )
         //EventOrAPT: Appointment or Event to draw
         
         // => SourceToScreen
         STC_Details 						= gEventOrAPT..Content
         STC_APTTitle 					= gEventOrAPT..Title
         STC_BackgroundAPTTitle..BrushColor	= gEventOrAPT..BrushColor
         IMG_Image1..Value 				= gEventOrAPT..Image
         IMG_Important..Visible			= (gEventOrAPT..Importance <> 0)
         IMG_REPETITION..Visible			= gEventOrAPT..WithRepetition
         
         
         // if the ATP is small and if we have a logo
         IF MySelf..Height <= 2*STC_BackgroundAPTTitle..Height _AND_  gEventOrAPT..Image<>"" THEN
         	// place the caption beside the title
         	IMG_Image1..Y = STC_APTTitle..Y
         	STC_APTTitle..X = IMG_Image1..X + IMG_Image1..Width
         END
      -
        type : 15
