     code : |1+
      PROCEDURE RPT_SCHEDULE(LOCAL gdStartDate is date, LOCAL gdEndDate is date)
      
      FOR EACH TASK 
      	bOK is boolean = False
      	
      	// If the start date of the task is smaller than the start date, then the end date must be greater than the start date
      	IF TASK.STARTDATE <= gdStartDate AND TASK.ENDDATE >= gdStartDate THEN
      		bOK = True
      	END
      	
      	// If the start date of the task is greater than the start date, then the start date of the task must be smaller than the end date
      	IF TASK.STARTDATE >= gdStartDate AND TASK.STARTDATE <= gdEndDate THEN
      		bOK = True
      	END
      	
      	IF bOK THEN
      		// Variable
      		dStartDate is date = TASK.STARTDATE
      		dEndDate   is date = TASK.ENDDATE
      		
      		IF TASK.STARTDATE < gdStartDate THEN
      			dStartDate = gdStartDate
      		END
      		
      		IF TASK.ENDDATE > gdEndDate THEN
      			dEndDate = gdEndDate
      		END
      		
      		HReadSeekFirst(PERSON,PERSONID,TASK.PERSONID)
      		HReadSeekFirst(TYPE,TYPEID,TASK.TYPEID)
      		
      		SchedulerAddAppointment(SCH_SCHEDULE,PERSON.NAME,TASK.STCTASK,dStartDate,dEndDate,TYPE.STCTYPE)
      	END	
      END
      
      
