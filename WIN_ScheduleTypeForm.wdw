     p_codes :
      -
        code : |1+
         
         // Variables
         TVRoot is string = <§$0000§>
         TVBranch is string
         
         // Initialization
         TreeAdd(TREE_Person,TVRoot,"GROUP.ICO","GROUP.ICO","0")
         
         // Groups of people
         i=0
         LOOP
         	i++
         	HRead(GROUPS,i)
         	IF HOut() THEN BREAK
         	IF (HState()=hStateActive) THEN
                 // Create the group in the TreeView
         		TVBranch=NoSpace(GROUPS.STCGROUP)
         		TreeAdd(TREE_Person,TVRoot+TAB+TVBranch,tvDefault,tvDefault,"G"+GROUPS.GROUPID)
         		TreeExpand(TREE_Person,TVRoot+TAB+TVBranch)
         		// Find the persons assigned to this group
         		HReadSeek(PERSON,GROUPID,GROUPS.GROUPID)
         		WHILE HFound()
         			TreeAdd(TREE_Person,TVRoot+TAB+TVBranch+TAB+NoSpace(PERSON.NAME),"NEXT5-.BMP","NEXT5-.BMP","P"+PERSON.PERSONID)
         			HReadNext(PERSON,GROUPID)
         		END
         	END
         END
         
        type : 14
