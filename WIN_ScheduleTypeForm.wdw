   -
     code : |1+
      PROCEDURE WIN_ScheduleTypeForm(sFrom="",sTo="",sListWho="")
      
      GLOBAL
      	i is int
      	s is string
      	nPersonID is int  
      
      // Selected persons
      IF NOT sListWho~=""
      	LOOP
      		i++
      		s=ExtractString(sListWho,i)
      		IF s=EOT THEN BREAK
      		nPersonID=Val(s)
      		HReadSeek(PERSON,PERSONID,nPersonID)
      		IF HFound() THEN ListAdd(LIST_Person,PERSON.NAME+RepeatString(" ",100)+TAB+PERSON.PERSONID)
      	END
      END
      
      
      // Default period
      IF sFrom~="" THEN sFrom=DateSys()
      IF sTo~="" THEN sTo=IntegerToDate(DateToInteger(sFrom)+7)
