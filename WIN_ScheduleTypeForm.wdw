     p_codes :
      -
        code : |1+
         // Retrieve the ID of the selected branch
         ID is string = TreeIdentifier(TREE_Person,TreeSelect(TREE_Person))
         SWITCH ID[[1]]
         	CASE "0"	// All
         		ListDeleteAll(LIST_Person)
         		i=0
         		LOOP
         			i++
         			HRead(PERSON,i)
         			IF HOut() THEN BREAK
         			IF (HState()=hStateActive) THEN ListAdd(LIST_Person,PERSON.NAME+RepeatString(" ",100)+TAB+PERSON.PERSONID)
         		END
         	CASE "G"	// Group
         		HReadSeek(PERSON,GROUPID,Val(ID[[2TO]]))
         		WHILE HFound()
         			IF ListSeek(LIST_Person,PERSON.NAME+RepeatString(" ",100)+TAB+PERSON.PERSONID)=-1 THEN
         				ListAdd(LIST_Person,PERSON.NAME+RepeatString(" ",100)+TAB+PERSON.PERSONID)
         			END
         			HReadNext(PERSON,GROUPID)
         		END
         	CASE "P"	// Person
         		HReadSeek(PERSON,PERSONID,Val(ID[[2TO]]))
         		IF ListSeek(LIST_Person,PERSON.NAME+RepeatString(" ",100)+TAB+PERSON.PERSONID)=-1 THEN
         			ListAdd(LIST_Person,PERSON.NAME+RepeatString(" ",100)+TAB+PERSON.PERSONID)
         		END
         	OTHER CASE
         		// Not managed
         END
         
